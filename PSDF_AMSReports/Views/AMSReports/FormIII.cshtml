<body>
    <p><strong>Punjab Skills Development Fund</strong></p>
    <p><strong>Form III - Monitoring Visit Report of Class</strong></p>
    
    <table class="tg_tsp" style="width:100%">
        <tbody>
            <tr>
                <td class="tg_tsp-axxb">Service Provider Name</td>
                <td class="tg_tsp-hle0">@Model.TSPDetails?.TSPName</td>
                <td class="tg_tsp-axxb">Training Centre (Name/Title)</td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Visit in Month</td>
                <td class="tg_tsp-0pky">@Model.VisitCount</td>
                <td class="tg_tsp-0pky">@Model.TSPDetails?.CentreName</td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Training Scheme</td>
                <td class="tg_tsp-0pky">@Model.TSPDetails?.SchemeName</td>
                <td class="tg_tsp-0pky"></td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Class Code</td>
                <td class="tg_tsp-0pky">@Model.TSPDetails?.ClassCode</td>
                <td class="tg_tsp-0pky"></td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Trade</td>
                <td class="tg_tsp-0pky">@Model.TSPDetails?.TradeName</td>
                <td class="tg_tsp-0pky"></td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Instructor/Trainer Name</td>
                <td class="tg_tsp-0pky">@Model.TSPDetails?.InstructorsInfo</td>
                <td class="tg_tsp-0pky"></td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Reporting Month</td>
                <td class="tg_tsp-0pky">@Model.TSPDetails?.ReportingMonth</td>
                <td class="tg_tsp-0pky"></td>
            </tr>
        </tbody>
    </table>
    <br /><br />
    <table class="tg_tsp" style="width:100%">
        <tbody>
            <tr>
                <td class="tg_tsp-axxb">Centre Related Information</td>
                <td class="tg_tsp-axxb">Visit-1</td>
                <td class="tg_tsp-axxb">Visit-2</td>
                <td class="tg_tsp-axxb">Visit-3</td>
                <td class="tg_tsp-axxb">Visit-4</td>
                <td class="tg_tsp-axxb">Remarks</td>
            </tr>
            @*@foreach (var item in Model.VisitsList)
            {
                <tr>
                    <td class="tg_tsp-0pky">Is the centre locked/non-functional?</td>
                    <td class="tg_tsp-0pky">@item.IsLock</td>
                </tr>
            }
            @foreach (var item in Model.VisitsList)
            {
                <tr>
                    <td class="tg_tsp-0pky">Has the centre been relocated unannounced?</td>
                    <td class="tg_tsp-0pky">@item.IsRelocated</td>
                </tr>
            }
            @foreach (var item in Model.VisitsList)
            {
                <tr>
                    <td class="tg_tsp-0pky">Is the lab equipment/consumables required for the training are available?</td>
                    <td class="tg_tsp-0pky">@item.EquipmentAvailable</td>
                </tr>
            }
            @foreach (var item in Model.VisitsList)
            {
                <tr>
                    <td class="tg_tsp-0pky">
                        Is the trainer/instructor delivering the lecture is the same person as
                        mentioned in the profile provided by the TSP to PSDF?
                    </td>
                    <td class="tg_tsp-0pky">@item.SameInstructor</td>
                </tr>
            }*@
            <tr>
                <td class="tg_tsp-0pky">Is the centre locked/non-functional?</td>
                <td class="tg_tsp-0pky">@Model.Visit1?.IsLock</td>
                <td class="tg_tsp-0pky">@Model.Visit2?.IsLock</td>
                <td class="tg_tsp-0pky">@Model.Visit3?.IsLock</td>
                <td class="tg_tsp-0pky">@Model.Visit4?.IsLock</td>
                <td class="tg_tsp-0pky">
                    <span>@Model.Visit1?.IsLockRemarks</span>
                    <span>@Model.Visit2?.IsLockRemarks</span>
                    <span>@Model.Visit3?.IsLockRemarks</span>
                    <span>@Model.Visit4?.IsLockRemarks</span>
                </td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Has the centre been relocated unannounced?</td>
                <td class="tg_tsp-0pky">@Model.Visit1?.IsRelocated</td>
                <td class="tg_tsp-0pky">@Model.Visit2?.IsRelocated</td>
                <td class="tg_tsp-0pky">@Model.Visit3?.IsRelocated</td>
                <td class="tg_tsp-0pky">@Model.Visit4?.IsRelocated</td>
                <td class="tg_tsp-0pky">
                    <span>@Model.Visit1?.IsLockRemarks</span>
                    <span>@Model.Visit2?.IsLockRemarks</span>
                    <span>@Model.Visit3?.IsLockRemarks</span>
                    <span>@Model.Visit4?.IsLockRemarks</span>
                </td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">Is the lab equipment/consumables required for the training are available?</td>
                <td class="tg_tsp-0pky">@Model.Visit1?.EquipmentAvailable</td>
                <td class="tg_tsp-0pky">@Model.Visit2?.EquipmentAvailable</td>
                <td class="tg_tsp-0pky">@Model.Visit3?.EquipmentAvailable</td>
                <td class="tg_tsp-0pky">@Model.Visit4?.EquipmentAvailable</td>
                <td class="tg_tsp-0pky">
                    <p>@Model.Visit1?.IsRelocatedRemarks</p>
                    <p>@Model.Visit2?.IsRelocatedRemarks</p>
                    <p>@Model.Visit3?.IsRelocatedRemarks</p>
                    <p>@Model.Visit4?.IsRelocatedRemarks</p>
                </td>
            </tr>
            <tr>
                <td class="tg_tsp-0pky">
                    Is the trainer/instructor delivering the lecture is the same person as
                    mentioned in the profile provided by the TSP to PSDF?
                </td>

                <td class="tg_tsp-0pky">@Model.Visit1?.SameInstructor</td>
                <td class="tg_tsp-0pky">@Model.Visit2?.SameInstructor</td>
                <td class="tg_tsp-0pky">@Model.Visit3?.SameInstructor</td>
                <td class="tg_tsp-0pky">@Model.Visit4?.SameInstructor</td>
                <td class="tg_tsp-0pky">
                    <p>@Model.Visit1?.IsRelocatedRemarks</p>
                    <p>@Model.Visit2?.IsRelocatedRemarks</p>
                    <p>@Model.Visit3?.IsRelocatedRemarks</p>
                    <p>@Model.Visit4?.IsRelocatedRemarks</p>
                </td>
            </tr>
        </tbody>
    </table>
