    <br /><br />
    <table class="tg_cmr" style="width:100%;">
        <tbody>
            <tr>
                <td class="tg_cmr-axxb" colspan="5">CLASS MONITORING REPORT</td>
            </tr>
            <tr>
                <td class="tg_cmr-6e8n" colspan="5">Total Violations in the Month</td>
            </tr>
            <tr>
                <td class="tg_cmr-6e8n">Minor Violations</td>
                <td class="tg_cmr-zv36">@Model.ClassViolations?.minor_count</td>
                <td class="tg_cmr-6e8n" colspan="2">Major Violations</td>
                <td class="tg_cmr-zv36">@Model.ClassViolations?.major_count</td>
            </tr>
            <tr>
                <td class="tg_cmr-6e8n">Serious Violations</td>
                <td class="tg_cmr-zv36">@Model.ClassViolations?.minor_count</td>
                <td class="tg_cmr-6e8n" colspan="2">Observations</td>
                <td class="tg_cmr-zv36">@Model.ClassViolations?.observation_count</td>
            </tr>
            <tr>
                <td class="tg_cmr-zlqz" colspan="5">Visit Detail</td>
            </tr>
            <tr>
                <td class="tg_tsp-7btt">Visit</td>
                <td class="tg_tsp-7btt">Visit-1</td>
                <td class="tg_tsp-7btt">Visit-2</td>
                <td class="tg_tsp-7btt">Visit-3</td>
                <td class="tg_tsp-7btt">Visit-4</td>
            </tr>
            <tr>
                <td class="tg_cmr-0pky">Visit Date</td>
                <td class="tg_cmr-0pky">@Model.CMVisit1?.VDate</td>
                <td class="tg_cmr-0pky">@Model.CMVisit2?.VDate</td>
                <td class="tg_cmr-0pky">@Model.CMVisit3?.VDate</td>
                <td class="tg_cmr-0pky">@Model.CMVisit4?.VDate</td>
            </tr>
            <tr>
                <td class="tg_cmr-0pky">Visit Time</td>
                <td class="tg_cmr-0pky">@Model.CMVisit1?.VTime</td>
                <td class="tg_cmr-0pky">@Model.CMVisit2?.VTime</td>
                <td class="tg_cmr-0pky">@Model.CMVisit3?.VTime</td>
                <td class="tg_cmr-0pky">@Model.CMVisit4?.VTime</td>
            </tr>
            <tr>
                <td class="tg_cmr-0pky">Number of trainees present at time of visit</td>
                <td class="tg_cmr-0pky">@Model.CMVisit1?.TraineesPresentCount</td>
                <td class="tg_cmr-0pky">@Model.CMVisit2?.TraineesPresentCount</td>
                <td class="tg_cmr-0pky">@Model.CMVisit3?.TraineesPresentCount</td>
                <td class="tg_cmr-0pky">@Model.CMVisit4?.TraineesPresentCount</td>
            </tr>
            <tr>
                <td class="tg_cmr-0pky">
                    Number of trainees marked present in the attendance register:
                    One day before the visit
                </td>
                <td class="tg_cmr-0pky">@Model.CMVisit1?.TraineesCountOne</td>
                <td class="tg_cmr-0pky">@Model.CMVisit2?.TraineesCountOne</td>
                <td class="tg_cmr-0pky">@Model.CMVisit3?.TraineesCountOne</td>
                <td class="tg_cmr-0pky">@Model.CMVisit4?.TraineesCountOne</td>
            </tr>
            <tr>
                <td class="tg_cmr-0pky">
                    Number of trainees marked present in the attendance register:
                    Two days before the visit
                </td>
                <td class="tg_cmr-0pky">@Model.CMVisit1?.TraineesCountTwo</td>
