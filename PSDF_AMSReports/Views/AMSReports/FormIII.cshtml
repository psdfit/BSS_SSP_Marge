            </tr>
            <tr>
                <td class="tg_pot-kmbl">Sr No</td>
                <td class="tg_pot-kmbl">Description</td>
                @for (int i = 1; i < Model.VisitCount + 1; i++)
                {
                    <td class="tg_pot-kmbl">Visit-@i</td>
                }
            </tr>
            <tr>
                <td class="tg_pot-kmbl"></td>
                <td class="tg_pot-kmbl"></td>
                @for (int i = 1; i < Model.VisitCount + 1; i++)
                {
                    <td class="tg_pot-kmbl">% Satisified</td>
                }
            </tr>
            @if (Model.TraineesPreceptions != null)
            {
                var tpcount = 1;
                @foreach (var item in Model.TraineesPreceptions?.GroupBy(x => x.VioID))
                {
                    <tr>
                        <td class="tg_tsp-0pky">@tpcount</td>
                        <td class="tg_tsp-0pky">@item.First().VioDesc</td>
                        @foreach (var rec in item)
                        {
                            <td class="tg_tsp-0pky">@rec.PercentageSatisfied</td>
                        }
                    </tr>
                    tpcount++;
                }
            }

        </tbody>
    </table>
    <br /><br />
    <table class="tg_viot" style="width:100%;">
        <tbody>
            <tr>
                <td class="tg_viot-kmbl">Sr No</td>
                <td class="tg_viot-kmbl">Parameters</td>
                <td class="tg_viot-kmbl">Violation Type</td>
                <td class="tg_viot-kmbl" colspan="4">Violation</td>
            </tr>
            <tr>
                <td class="tg_viot-kmbl"></td>
                <td class="tg_viot-kmbl"></td>
                <td class="tg_viot-kmbl"></td>
                @for (int i = 1; i < Model.VisitCount + 1; i++)
                {
                    <td class="tg_viot-kmbl">Visit-@i</td>
                }
            </tr>
            <tr>
                <td class="tg_viot-kmbl"></td>
                <td class="tg_viot-kmbl"></td>
                <td class="tg_viot-kmbl"></td>
                @for (int i = 1; i < Model.VisitCount + 1; i++)
                {
                    <td class="tg_viot-kmbl">Yes/No</td>
                }
            </tr>

            
            @if (Model.ViolationTypeList != null)
            {
                var count = 1;
                @foreach (var item in Model.ViolationTypeList.GroupBy(x => x.VioID))
                {
                    <tr>
                        <td class="tg_tsp-0pky">@count</td>
                        <td class="tg_tsp-0pky">@item.First().VioDesc</td>
                        <td class="tg_tsp-0pky">@item.First().ViolationType</td>
                        @foreach (var rec in item)
                        {
                            <td class="tg_tsp-0pky">@rec.IsViolation</td>
                        }
                    </tr>
                    count++;
                }
            }

        </tbody>
    </table>
    <br /><br />
    <table class="tg_rem" style="width:100%;">
        <tbody>
            <tr>
                <td class="tg_rem-1wig" colspan="2">20. Any other observation by the Monitoring Officer or feedback from the trainees ?</td>
            </tr>
            <tr>
                <td class="tg_rem-0lax">I.</td>
                <td class="tg_rem-0lax">FM Remarks: @Model.Visit1?.FMSignOffRemarks<br>FM Remarks on Trainees Feedback: @Model.Visit1?.TspSignOffRemarks</td>
            </tr>
            <tr>
                <td class="tg_rem-0lax">II.</td>
                <td class="tg_rem-0lax">FM Remarks: @Model.Visit2?.FMSignOffRemarks<br>FM Remarks on Trainees Feedback: @Model.Visit2?.TspSignOffRemarks</td>
            </tr>
            <tr>
                <td class="tg_rem-0lax">III.</td>
                <td class="tg_rem-0lax">FM Remarks: @Model.Visit3?.FMSignOffRemarks<br>FM Remarks on Trainees Feedback: @Model.Visit3?.TspSignOffRemarks</td>
            </tr>
            <tr>
                <td class="tg_rem-0lax">IV.</td>
                <td class="tg_rem-0lax">FM Remarks: @Model.Visit4?.FMSignOffRemarks<br>FM Remarks on Trainees Feedback: @Model.Visit4?.TspSignOffRemarks</td>
            </tr>
        </tbody>
    </table>
    <br /><br /><br /><br />
    <table class="tg_tsp" style="width:100%">
        <tbody>
            <tr>
                <td class="tg_tsp-lgs3" colspan="5">V. Details of Added/Deleted/Dropout trainees in attendance register</td>
            </tr>
            <tr>
                <td class="tg_tsp-7btt">Sr No.</td>
                <td class="tg_tsp-7btt">Trainee Name</td>
                <td class="tg_tsp-7btt">Father's/Husband's Name</td>
                <td class="tg_tsp-7btt">CNIC #</td>
                <td class="tg_tsp-7btt">Remarks</td>
            </tr>
            @if (Model.DeleteDropOutTrainees != null)
            {
                @foreach (var item in Model.DeleteDropOutTrainees)
                {
                    <tr>
                        <td class="tg_tsp-0pky">@item.SrNo</td>
                        <td class="tg_tsp-0pky">@item.DisplayName</td>
                        <td class="tg_tsp-0pky">@item.FatherName</td>
                        <td class="tg_tsp-0pky">@item.TraineeCNIC</td>
                        <td class="tg_tsp-0pky">@item.Remarks</td>
                    </tr>
                }
            }


        </tbody>
    </table>
    <br /><br />
    <table class="tg_sign" style="width:100%">
        <tbody>
            <tr>
                <td class="tg_sign-u1yq">Signature By</td>
                <td class="tg_sign-ziz6">Visit-1</td>
                <td class="tg_sign-ziz6">Visit-2</td>
                <td class="tg_sign-ziz6">Visit-2</td>
                <td class="tg_sign-ziz6">Visit-4</td>
            </tr>
            <tr>
                <td class="tg_sign-abx8">Field Monitor</td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit1?.FMSignImage" /></td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit2?.FMSignImage" /></td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit3?.FMSignImage" /></td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit4?.FMSignImage" /></td>
            </tr>
            <tr>
                <td class="tg_sign-abx8">
                    TSP Representative
                </td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit1?.TSPSignImage" /></td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit2?.TSPSignImage" /></td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit3?.TSPSignImage" /></td>
                <td class="tg_sign-0lax"><img style="width:100px;" src="@Model.Visit4?.TSPSignImage" /></td>
            </tr>
        </tbody>
    </table>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</body>
</html>
